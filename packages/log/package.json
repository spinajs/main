{
  "name": "@spinajs/log",
  "version": "2.0.400",
  "description": "Log lib for all spinejs related libs",
  
    "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
      ".": {
          "import": "./lib/mjs/index.js",
          "require": "./lib/cjs/index.js"
      }
  },
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.mjs.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
    
  },
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/log)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/log/issues"
  },
  "homepage": "https://github.com/spinajs/log#readme",
  "dependencies": {
    "@colors/colors": "^1.5.0",
    "@spinajs/configuration": "^2.0.400",
    "@spinajs/di": "^2.0.400",
    "@spinajs/exceptions": "^2.0.400",
    "@spinajs/internal-logger": "^2.0.400",
    "@spinajs/log-common": "^2.0.400",
    "ajv": "^8.12.0",
    "chalk": "5.2.0",
    "glob": "^8.1.0",
    "glob-to-regexp": "^0.4.1",
    "lodash": "^4.17.21",
     "luxon": "^3.6.1",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "@types/glob": "8.0.1",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/mock-fs": "^4.13.1",
    "@types/node-schedule": "^2.1.0",
    "mock-fs": "^5.2.0"
  },
  "gitHead": "002dc553b0ffffd72193d0121ac425a4083bc9ee"
}
