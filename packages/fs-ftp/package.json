{
  "name": "@spinajs/fs-ftp",
  "version": "2.0.392",
  "description": "file operations implementation for ftp",
  
    "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
      ".": {
          "import": "./lib/mjs/index.js",
          "require": "./lib/cjs/index.js"
      }
  },
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "scripts": {
    "watch": "tsc -b tsconfig.mjs.json -watch",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.mjs.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
    
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/exceptions.git"
  },
  "keywords": [
    "di",
    "container",
    "spinajs",
    "di"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/exceptions/issues"
  },
  "homepage": "https://github.com/spinajs/exceptions#readme",
  "gitHead": "002dc553b0ffffd72193d0121ac425a4083bc9ee",
  "devDependencies": {
    "@types/archiver": "^5.3.1"
  },
  "dependencies": {
    "@spinajs/configuration": "^2.0.392",
    "@spinajs/di": "^2.0.392",
    "@spinajs/fs": "^2.0.392",
    "@spinajs/log": "^2.0.392",
    "basic-ftp": "^5.0.5",
    "lodash": "^4.17.21",
     "luxon": "^3.6.1"
  }
}
