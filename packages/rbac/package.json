{
  "name": "@spinajs/rbac",
  "version": "2.0.389",
  "description": "Role and Attribute based Access Control for SpinaJS framework",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.mjs.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/main.git"
  },
  "keywords": [
    "spinajs",
    "rbac"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/main)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/main/issues"
  },
  "homepage": "https://github.com/spinajs/main#readme",
  "dependencies": {
    "@spinajs/configuration": "^2.0.389",
    "@spinajs/di": "^2.0.389",
    "@spinajs/exceptions": "^2.0.389",
    "@spinajs/log": "^2.0.389",
    "@spinajs/orm": "^2.0.389",
    "@spinajs/queue": "^2.0.389",
    "@spinajs/reflection": "^2.0.389",
    "@spinajs/util": "^2.0.389",
    "@spinajs/validation": "^2.0.389",
    "accesscontrol": "^2.2.1",
    "argon2": "^0.30.3",
    "cron": "^2.2.0",
    "entropy-string": "^4.2.0",
    "lodash": "^4.17.21",
     "luxon": "^3.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@spinajs/orm-sqlite": "^2.0.389",
    "@types/cron": "^2.0.0",
    "@types/entropy-string": "^4.2.0"
  },
  "gitHead": "002dc553b0ffffd72193d0121ac425a4083bc9ee"
}
