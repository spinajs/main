{
  "name": "@spinajs/fs",
  "version": "2.0.321",
  "description": "wrapper for file operations",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "scripts": {

    "watch": "tsc -b tsconfig.mjs.json -watch",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.mjs.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/exceptions.git"
  },
  "keywords": [
    "di",
    "container",
    "spinajs",
    "di"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/exceptions/issues"
  },
  "homepage": "https://github.com/spinajs/exceptions#readme",
  "gitHead": "002dc553b0ffffd72193d0121ac425a4083bc9ee",
  "dependencies": {
    "@spinajs/exceptions": "2.0.321",
    "@spinajs/util": "2.0.321",
    "@spinajs/log-common": "2.0.321", 
    "archiver": "^5.3.1",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "uuid": "^9.0.0",
    "exiftool": "0.0.3",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/luxon": "^3.2.0",
    "@types/uuid": "^9.0.0",
    "@types/unzipper":"^0.10.7"
  }
}
