{
  "name": "@spinajs/configuration",
  "version": "2.0.256",
  "description": "framework configuration module",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.mjs.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/configuration.git"
  },
  "keywords": [
    "di",
    "container",
    "spinajs",
    "di"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/core)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/configuration/issues"
  },
  "homepage": "https://github.com/spinajs/configuration#readme",
  "devDependencies": {
    "@types/ajv-merge-patch": "^4.1.2"
  },
  "dependencies": {
    "@spinajs/configuration-common": "^2.0.256",
    "@spinajs/di": "^2.0.256",
    "@spinajs/exceptions": "^2.0.256",
    "@spinajs/internal-logger": "^2.0.256",
    "@spinajs/util": "2.0.256",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "ajv-merge-patch": "^5.0.1",
    "glob": "^8.1.0",
    "lodash": "^4.17.21"
  },
  "gitHead": "002dc553b0ffffd72193d0121ac425a4083bc9ee"
}
