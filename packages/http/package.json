{
  "name": "@spinajs/http",
  "version": "2.0.392",
  "description": "framework HTTP module base on express.js",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "scripts": {
    "watch": "tsc -b tsconfig.mjs.json -w",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.mjs.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/main.git"
  },
  "keywords": [
    "spinajs",
    "http"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/main)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/main/issues"
  },
  "homepage": "https://github.com/spinajs/main#readme",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-signature": "^1.1.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^5.0.1",
    "@types/formidable": "^2.0.5",
    "@types/luxon": "^3.6.1",
    "@types/node": "^22.14.1",
    "@types/pug": "^2.0.6",
    "@types/uuid": "^9.0.0",
    "cpx": "1.5.0"
  },
  "dependencies": {
    "@spinajs/configuration": "^2.0.392",
    "@spinajs/di": "^2.0.392",
    "@spinajs/exceptions": "^2.0.392",
    "@spinajs/fs": "^2.0.392",
    "@spinajs/log": "^2.0.392",
    "@spinajs/reflection": "^2.0.392",
    "@spinajs/templates": "^2.0.392",
    "@spinajs/validation": "^2.0.392",
    "@spinajs/templates-pug": "^2.0.392",
    "@spinajs/util": "^2.0.392",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-signature": "^1.2.0",
    "cors": "^2.8.5",
    "csv": "^6.2.5",
    "express": "^5.1.0",
    "formidable": "^3.2.5",
    "glob": "^8.1.0",
    "helmet": "^6.0.1",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "mime": "^3.0.0",
    "randomstring": "^1.2.3",
    "uuid": "^9.0.0"
  },
  "gitHead": "002dc553b0ffffd72193d0121ac425a4083bc9ee"
}
