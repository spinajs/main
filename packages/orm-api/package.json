{
  "name": "@spinajs/orm-api",
  "version": "2.0.393",
  "description": "Orm extensions for HTTP module",
  "main": "lib/cjs/index.js",
  "module": "lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=16.11"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "tsc -b tsconfig.mjs.json",
    "compile:cjs": "tsc -b tsconfig.cjs.json",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf lib/ && rimraf tsconfig.tsbuildinfo",
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts",
    "coverage": "nyc npm run test",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.cjs --ext .ts src --fix",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinajs/main.git"
  },
  "keywords": [
    "spinajs",
    "rbac"
  ],
  "author": "SpinaJS <spinajs@coderush.pl> (https://github.com/spinajs/main)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spinajs/main/issues"
  },
  "homepage": "https://github.com/spinajs/main#readme",
  "dependencies": {
    "@spinajs/configuration": "^2.0.393",
    "@spinajs/di": "^2.0.393",
    "@spinajs/exceptions": "^2.0.393",
    "@spinajs/http": "^2.0.393",
    "@spinajs/intl-orm": "^2.0.393",
    "@spinajs/log": "^2.0.393",
    "@spinajs/orm": "^2.0.393",
    "@spinajs/validation": "2.0.393",
    "@spinajs/rbac": "^2.0.393",
    "@spinajs/rbac-http": "^2.0.393",
    "lodash": "^4.17.21",
     "luxon": "^3.6.1"
  },
  "devDependencies": {
    "@spinajs/orm-sqlite": "^2.0.393"
  },
  "gitHead": "b0f038aa2814b69b242ab9a277b26d73d39544d2"
}
